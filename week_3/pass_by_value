#include <iostream>
using namespace std;
void passByValue(int x);
int main()
{
    int x = 3;
    cout <<"Addres " <<&x <<"\n";
    cout <<"Value " <<x<<"\n";
    
    passByValue(x);
    cout <<"Addres " <<&x <<"\n";
    cout <<"Value " <<x<<"\n";
    
    return 0;
}
void passByValue(int x)
{
    cout <<"Addres " <<&x <<"\n";
    cout <<"Value " <<x<<"\n";
    x=4;
    cout <<"Addres " <<&x <<"\n";
    cout <<"Value " <<x<<"\n";
    cout<<"----- passValue end ------";
}
